   "source": [
    "#Read two integer numbers and check if first is multiple of second\n",
    "a=int(input('Enter First number: '))\n",
    "b=int(input('Enter Second number: '))\n",
    "remainder=a%b\n",
    "if remainder==0:\n",
    "    print(a,' is multiple of ',b)\n",
    "else:\n",
    "    print(a,' is not multiple of ',b)"
